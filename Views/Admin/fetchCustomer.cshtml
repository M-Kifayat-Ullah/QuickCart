@model List<Customer>
<div class="container-fluid px-4">
    <h2 class="mt-4 text-center text-uppercase" style="letter-spacing:4px">My Dashboard</h2>
</div>
<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Customer Name</th>
                    <th>Customer Phone</th>
                    <th>Customer Country</th>
                    <th>Customer City</th>
                    <th>Customer Address</th>

                </tr>
            </thead>
            <tbody>
                @foreach(var cust in Model)
                {
                    <tr>
                       <td>@cust.Customer_name</td>
                        <td>@cust.Customer_phone</td>
                        <td>@cust.Customer_country</td>
                        <td>@cust.Customer_city</td>
                        <td>@cust.Customer_address</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>